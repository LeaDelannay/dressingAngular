$(document).ready(function($) {
    $(document).on('click', '.alert-warning a', function(e) {
        e.preventDefault();
        animateScrollTo( $($(this).attr('href')).offset().top - 120, 700);
    });

    // Gestion multi-popins
	$('#PopinOui, #PopinNon').on('hidden.bs.modal', function () {
		if ($('.modal.in').length) { $('body').addClass('modal-open'); }
	});
});
//# sourceMappingURL=data:application/json;charset=utf8;base64,
